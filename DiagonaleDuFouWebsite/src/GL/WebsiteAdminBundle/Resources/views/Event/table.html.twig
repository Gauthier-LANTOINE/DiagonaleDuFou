<table class="table table-bordered table-striped text-center table-admin ">
            <tbody>
                {% for event in listEvents %}
                <tr>
                    
                    <td>{{ event.name }}</td>
                    <td>{{ event.category.name }}</td>
                    <td>{{ event.date|date("d/m/Y") }}</td>    
                </tr>
                {% endfor %}

            </tbody>
        </table>
